# AI Services
OPENAI_API_KEY=your_openai_development_api_key
DEEPSEEK_API_KEY=your_deepseek_development_api_key
KIMI_API_KEY=your_kimi_development_api_key

# Vector Store
VECTOR_DB_TYPE=faiss
VECTOR_STORE_PATH=./vector_store

# Document Processing
MAX_FILE_SIZE=10
CHUNK_SIZE=1000
CHUNK_OVERLAP=200

# LLM Configuration
DEFAULT_MODEL=gpt-3.5-turbo
TEMPERATURE=0.7
MAX_TOKENS=4000

# Logging
LOG_LEVEL=DEBUG
LOG_FILE=app.log

# UI Configuration
THEME=light
PAGE_TITLE=Document Chat Assistant - Development

# Security
SECRET_KEY=dev_secret_key_here
ALLOWED_HOSTS=localhost,127.0.0.1
SECURE_SSL_REDIRECT=false
SESSION_COOKIE_SECURE=false
CSRF_COOKIE_SECURE=false

# Database (if using)
DATABASE_URL=sqlite:///./dev.db
REDIS_URL=redis://localhost:6379/0

# Monitoring
SENTRY_DSN=
PROMETHEUS_METRICS_ENABLED=false

# Analytics
GOOGLE_ANALYTICS_ID=
VERCEL_ANALYTICS_ID=

# Development Settings
DEBUG=true
RELOAD=true
LOG_TO_CONSOLE=true